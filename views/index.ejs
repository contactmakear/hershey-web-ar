  <script>
    $("p.expansion-btn").click(function () {
      classes = this.className;
      var divNumber = classes.slice(-1);
      var toGetId = "#div-" + divNumber;
      if ($(toGetId).hasClass("expanded-div")) {
        $(".normal-div").removeClass("compressed-div expanded-div");
      }
      else {
        $(".normal-div").removeClass("compressed-div expanded-div").addClass("compressed-div");;
        $(toGetId).removeClass("compressed-div").addClass("expanded-div");
      }
    });


    // -------------------------------card
    $("p.custom-expansion-btn").click(function () {
  classes = this.className;
  var divNumber = classes.slice(-1);
  var toGetId = "#custom-div-" + divNumber;

  if ($(toGetId).hasClass("custom-expanded-div")) {
    $(".custom-div").removeClass("custom-compressed-div custom-expanded-div");
  } else {
    $(".custom-div").removeClass("custom-compressed-div custom-expanded-div").addClass("custom-compressed-div");
    $(toGetId).removeClass("custom-compressed-div").addClass("custom-expanded-div");
  }
});
// -----------------------------------------------
$(document).ready(function() {
$("p.custom-option-btn").click(function() {
  var classes = this.className;
  var divNumber = classes.slice(-1);
  var toGetId = "#custom-option-" + divNumber;
  if ($(toGetId).hasClass("custom-expanded-option")) {
    $(".custom-option").removeClass("custom-compressed-option custom-expanded-option");
  } else {
    $(".custom-option").removeClass("custom-compressed-option custom-expanded-option").addClass("custom-compressed-option");
    $(toGetId).removeClass("custom-compressed-option").addClass("custom-expanded-option");
  }
});
});

  </script>
