  <!-- Some UI for the user to tap to indicate they're happy with the placement -->
  <div id="zappar-placement-ui">Tap here to place the object</div>


  <a-scene id="scenediv" swap-texture renderer="antialias: true;
  colorManagement: true;
  sortObjects: true;
  physicallyCorrectLights: true;
  maxCanvasWidth: 1920;
  maxCanvasHeight: 1920;" gesture-detector>
    <!-- <canvas id="div">

  </canvas> -->
    <a-assets>

      <a-asset-item id="tree" src="/models/kisses .glb"></a-asset-item>
    </a-assets>
    <!-- Ask user for camera permissions, display some text if permission is denied -->
    <a-entity  id="permissions"></a-entity>
    <!-- shows a full-page dialog that informs the user they're using an unsupported browser -->
    <a-entity zappar-compatibility-ui id="compatibility"></a-entity>
    <!-- <canvas id="div"></canvas> -->
    <a-camera  id="camera" position="0 8 8" raycaster="objects: .cantap" cursor="
    fuse: false;
    rayOrigin: mouse;" zappar-camera />

    <!-- Setup our instant tracker and start it in placement mode so the user can -->
    <!-- choose a location for the object -->
    <a-entity zappar-instant="placement-mode: true" id="instant-tracker" >
      <a-entity id="modelname" gltf-model="#tree" class="cantap" visible="true" scale="0.1 0.1 0.1" position="0 -2 0"
         gesture-handler="minScale: 0.25; maxScale: 10"  animation-mixer="clip: Animation; loop: repeat"></a-entity>
      <!-- Include a 3D model inside our instant tracker -->
      <!-- <a-entity id="australiacont" gesture-handler gltf-model="url(assets/kisses .glb)" scale="0.1 0.1 0.1"  animation-mixer="clip: Animation; loop: repeat"></a-entity> -->
      <a-entity scale="7 7 7" light="type: directional;
          intensity: 0.8;
     shadowMapHeight:2048;
       shadowMapWidth:2048;
       shadowCameraTop: 10;
             target: #australiacont;" position="17.85 2.75 0.30" shadow>
      </a-entity>
      <a-entity scale="7 7 7" light="type: directional;
     intensity: 0.8;
    shadowMapHeight:2048;
    shadowMapWidth:2048;
    shadowCameraTop: 10;
        target: #australiacont;" position="-4.6 2.75 1.24" shadow>
      </a-entity>
      <a-entity scale="7 7 7" light="type: directional;
    intensity: 0.8;
    shadowMapHeight:2048;
    shadowMapWidth:2048;
    shadowCameraTop: 10;
       target: #australiacont;" position="3.43 2.68 -0.62" shadow>
      </a-entity>
      <a-entity scale="7 7 7" light="type: directional;
    intensity: 0.8;
    shadowMapHeight:2048;
    shadowMapWidth:2048;
    shadowCameraTop: 10;
       target: #australiacont;" position="0.52 2.62 12.75" shadow>
      </a-entity>
      <a-light type="ambient" intensity="2"></a-light>
    </a-entity>

  </a-scene>
  
